.mask{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  background: rgba(0, 0, 0, .4);
  z-index: 666;
}
.mask_box{
  width: 80%;
  height: 65%;
  border-radius: 10rpx;
  margin: 20% auto;
  background: #FFF;
  text-align: center;
  position: relative;
  z-index: 666;
}
.mask_box .wenan{
  width: 100%;
  height: calc(100% - 180rpx);
  overflow-x: scroll;
  z-index: 666;
}
.mask_box .button{
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #5a72ed;
  border-top: 1px solid #EEE;
  z-index: 666;
}
.wenan_top{
