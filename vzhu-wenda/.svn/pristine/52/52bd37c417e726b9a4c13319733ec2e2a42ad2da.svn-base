 @import "../common/common.wxss";   
.clear{
  clear: both;
}
.lf{
  float: left;
}
.rt{
  float: right;
}
.content{
  background: #fff;
  padding: 10px 10px;
  box-sizing: border-box;
}
.img_box{
  width: 130rpx;
  height: 130rpx;
  border-radius: 3rpx;
   margin-right: 20rpx;  
   float: left; 
}
.img_box image{
  width:100%;
  height: 100%;
}

