/* pages/renew/renew.wxss */
page{
    background: rgb(240,239,245);
}
.renew-item{
    display:flex;
    flex-direction:row;
    justify-content:space-between;
    padding:30rpx 0;
    margin-bottom:2rpx;
    background:#fff;
}
.renew-item-right{
    border:1rpx solid #4ba3f1;
    margin-right:5%;
    padding:10rpx 35rpx;
    color:#4ba3f1;
    width: 13%;
    text-align: center;
    border-radius: 3px;
}
.add-experience-navigator{
    width: 100%;
    position: absolute;
    bottom: 0;
    background: #fff;
    box-shadow:-3px -3px 3px #dadbdd;
}
.add-experience-content{
    text-align:center;
    font-size:36rpx;
    color:#fff;
    width:70%;
    margin:4% auto;
    line-height:90rpx;
    background:-webkit-linear-gradient(left, rgb(57,206,249), rgb(80,128,236));
    background:-o-linear-gradient(right, rgb(57,206,249), rgb(80,128,236));
    background:-moz-linear-gradient(right, rgb(57,206,249), rgb(80,128,236));
    background:linear-gradient(to right, rgb(57,206,249) , rgb(80,128,236)); 
    border-radius: 3px;
}