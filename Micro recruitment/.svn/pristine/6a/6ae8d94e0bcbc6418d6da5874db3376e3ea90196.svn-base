/* pages/company-apply-submit/company-apply-submit.wxss */
.nodata{
    width: 100%;
    height: 100%;
    background: #fff;
    position: relative;
}
.nodata image{
    width:50%;
    margin:0 auto;
    display:block;
    height:300rpx;
    padding:38% 0 0 0;
    padding-bottom:38%;
}
.nodata view{
  text-align:center;
  position:absolute;
  top:635rpx;
  left:30%;
  color:#c6c6c6;
}
.creat-btn{
    text-align:center;
    font-size:36rpx;
    color:#fff;
    width:80%;
    margin:0 auto;
    line-height:90rpx;
    background:-webkit-linear-gradient(left, rgb(57,206,249), rgb(80,128,236));
    background:-o-linear-gradient(right, rgb(57,206,249), rgb(80,128,236));
    background:-moz-linear-gradient(right, rgb(57,206,249), rgb(80,128,236));
    background:linear-gradient(to right, rgb(57,206,249) , rgb(80,128,236)); 
}