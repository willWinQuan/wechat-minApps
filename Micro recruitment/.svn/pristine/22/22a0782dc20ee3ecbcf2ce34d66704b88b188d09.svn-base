/* pages/send_msg/send_msg.wxss */
@import "../common/resume-list.wxss";

page{
    background: rgb(240,239,245); 
}
.heard-nav{
    background: #fff;
    padding-bottom: 25rpx;
}
.heard-nav text{
    margin: 0 20%; 
    padding-bottom:10rpx;
    font-size:33rpx;
}
.nodata{
    width: 100%;
    height: 100%;
    background: #fff;
}
.nodata image{
    width:50%;
    margin:0 auto;
    display:block;
    height:300rpx;
    padding:25% 0;
    padding-bottom:88%;
}
.myresume-personal-h {
    margin-bottom:20rpx;
    padding:6% 5%;
    background:#fff;
}

.myresume-personal-h image {
    width:18%;
    height: 120rpx;
}

.myresume-personal-hr {
    margin-left: 30rpx;
    margin-top: -10rpx;
    display:inline-block;
}

.myresume-personal-hrss { 
    font-size: 28rpx;
    padding:10rpx 0;
    color: #ccc;
    white-space:nowrap;
}

.myresume-personal-hrss text{
    color: #eee;
    padding: 0 10rpx;
}
.jiantou{
    width:5%;
    font-size:40rpx;
    text-align:right;
    color:#ccc;
    margin-top:2.5%;
    display:inline-block;
    float:right;
}
.nomore{
    font-size:30rpx;
    color:#999;
    text-align:center;
    padding-bottom:10rpx;
}