page{
  background: #f0eff5;
}
.integral_query{
  background: #ffffff;
  border-top: 1px solid #d9d9d9;
  border-bottom: 1px solid #d9d9d9;
  padding: 50rpx;
  text-align: center;
  margin-bottom: 50rpx;
}
.integral_text{
  font-size: 30rpx;
}
.integral{
  color: #3c94ff;
  font-size: 60rpx;
  font-weight: bold;
}
/*积分列表  */
.consume_record{
  padding: 20rpx;
  background: #ffffff;
  border-bottom: 1px solid #d9d9d9;
}
.consume_record:nth-child(2){
  border-top: 1px solid #d9d9d9;
}
.consume_left{
  width: 75%;
  display: inline-block;
  vertical-align: middle;
  font-size: 30rpx;
}
.consume_name{
  font-weight: bold;
}
.consume_time{
  color: #bababa;
}
.consume_right{
  width: 25%;
  display:inline-block;
  vertical-align: middle;
  text-align: right;
}
/*  加载更多   */
.loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.loadmore_wrap {
  width: 100%;
  padding: 30rpx 0;
  /* line-height: 30rpx; */
  font-size: 14px;
  text-align: center;
}
.loadmore {
  display: inline-block;
  vertical-align: middle;
}
/*没有记录  */
.no_record{
  width: 236rpx;
  height: 263rpx;
  display: block;
  margin: 0 auto;
}