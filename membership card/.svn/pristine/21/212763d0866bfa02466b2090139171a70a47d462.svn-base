/* pages/verification/verification.wxss */
page{
  background: #f0eff5;
}
.verification_date{
  padding: 20rpx;
  color: #bababa;
}
.client_record{
  background: #ffffff;
  padding: 20rpx;
  border-bottom: 1px solid #d9d9d9;
}
.store_name_wrap{
  font-weight: bold;
}
.store_name{
  width: 70%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: inline-block;
  vertical-align: middle;
}
.store_time{
  width: 30%;
  display: inline-block;
  vertical-align: middle;
  text-align: right;
}
.client_info{
  color: #bababa;
  font-size: 30rpx;
  padding:10rpx 0;
}
.client_name{
  width: 30%;
  display: inline-block;
  vertical-align: middle;
}
.client_card{
  width: 70%;
  display: inline-block;
  vertical-align: middle;
  text-align: right;
}
.client_time{
  width: 75%;
  display: inline-block;
  color: #bababa;
  font-size: 30rpx;
}
.client_way{
  width: 25%;
  display: inline-block;
  text-align: right;
}
/*  加载更多   */
.loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.loadmore_wrap {
  width: 100%;
  padding: 30rpx 0;
  /* line-height: 30rpx; */
  font-size: 14px;
  text-align: center;
}
.loadmore {
  display: inline-block;
  vertical-align: middle;
}
/*没有记录  */
.no_record{
  width: 236rpx;
  height: 263rpx;
  display: block;
  margin: 0 auto;
}