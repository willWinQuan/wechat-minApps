/* pages/alter_info/alter_info.wxss */
page{
  background: #f0eff5;
}
.alter_info{
  border-bottom: 1px solid #d9d9d9;
  padding: 30rpx 5%;
  background: #ffffff;
}
.alter_info:first-child{
  border-top: 1px solid #d9d9d9;  
}
.alter_info text{
  width: 95%;
  display: inline-block;
  vertical-align: middle;
}
.alter_info image{
  width: 23rpx;
  height: 38rpx;
  display: inline-block;
  vertical-align: middle;
}