page{
  background: #f0eff5;
}
.my_calculate_nav{
  border-top: 1px solid #d9d9d9;
  border-bottom:1px solid #d9d9d9;
  background: #ffffff;
}
.calc_usable,.calc_lose{
  width: 50%;
  padding: 30rpx 0;
  display: inline-block;
  box-sizing: border-box;
  text-align: center;
}
.select_present{
  /* color: #3c94ff; */
  border-bottom: 2px solid #3c94ff;
}
/*我的可用计次卡  */
.usable_list,.lose_list{
  padding: 20rpx;
  border-bottom: 1px solid #d9d9d9;
  position: relative;
  background: #ffffff;
}
.calc_card{
  width: 35%;
  display: inline-block;
  vertical-align: middle;
}
.calc_card image{
  width: 94%;
  height: 150rpx;
  border-radius:10rpx;
}
.calc_info{
  width: 60%;
  display: inline-block;
  vertical-align: middle;
  margin-left:1%;
}
.calc_name{
  font-weight: bold;
  margin-bottom:45rpx;
}
.calc_indate{
  font-size: 26rpx;
  color:#bababa;
  padding-top: 10rpx;
}
.calc_arrow{
  width:4%;
  display: inline-block;
  vertical-align: middle;
}
.calc_arrow image{
  width: 23rpx;
  height: 38rpx;
}
.calc_lose_image{
  position: absolute;
  top: 20rpx;
  right: 80rpx;
}
.calc_lose_image image{
  width: 128rpx;
  height: 100rpx;
}
.nodata{
  text-align: center;
  padding: 50rpx 0;
  color: #999;
}
/*  加载更多   */
.loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.loadmore_wrap {
  width: 100%;
  padding: 30rpx 0;
  /* line-height: 30rpx; */
  font-size: 14px;
  text-align: center;
}
.loadmore {
  display: inline-block;
  vertical-align: middle;
}