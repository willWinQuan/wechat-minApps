/* crowdfundingDetails.wxss */
page{
  font-family:"Microsoft YaHei",Arial,Helvetica,sans-serif;-webkit-text-size-adjust:none;
  overflow-x: hidden;
  background: rgb(245,245,245);
}
.clear{
	clear: both;
}
.header{
	width: 100%;
	background: #FFFFFF;
	margin-bottom:10rpx;
}
.header .li{
	font-size: 32rpx;
  color:#666;
	width: 33.333%;
	height: 60rpx;
	display: inline-block;
	float: left;
	line-height:60rpx;
	text-align: center;
}

.header .li .headerGr{
	display: inline-block;
	width: 30rpx;
	height: 30rpx;
	vertical-align:-3rpx;
   margin-right: 15rpx;
}
.header .li .headerYy{
	display: inline-block;
	width: 26rpx;
	height: 30rpx;
	vertical-align:-3rpx;
  margin-right: 15rpx;
}
.header .li .headerEm{
	display: inline-block;
	width: 30rpx;
	height: 30rpx;
	vertical-align:-6rpx;
  margin-right: 15rpx;
}
.header .headerImg{
	background: url(data:image/png;base64,) no-repeat 100% 50%;
  background-size: contain;
}
.explainC{
   padding-bottom:18%;
   margin-top:15rpx;
   background:#fff;
}
.nocontent{
  font-size:35rpx;
  text-align:center;
  color:#c9c9c9;
}
.nocontent .image{
  width:30%;
  height:138rpx;
  margin:0 auto;
  float: none;
}
/* 轮播 */
swiper{
  height: 380rpx;
}
.swiper image{
    display: inline;
}
.slideImage{
  width: 100%
}
/* 倒计时 */
.Countdown{
	font-size: 30rpx;
	color: #FFFFFF;
	background: #F4212B;
	text-align: center;
	margin-bottom: 10rpx;
}
.Countdown .time{
  display: inline-block;
	font-size: 30rpx;
	background: #9c1319;
	height:50rpx;
	width: 50rpx;
	line-height: 50rpx;
	margin: 20rpx 0;
}
/*  */
.product{
	width: 100%;
	background: #FFFFFF;
	margin-bottom: 20rpx;
}
.product .productId{
	width: 90%;
	margin-left: 5%;
	font-size:30rpx ;
	font-weight: 600;
	padding-top: 20rpx;
  margin-bottom: 10rpx;
}
.productMoney{
	margin-left: 5%;
	line-height: 50rpx;
}
.productMoney .Xj{
	font-size: 35.8rpx;
	color: #F4212B;
}
.productMoney .Yj{
	font-size: 25.8rpx;
	color: #999999;
  text-decoration:line-through;
}
.product .productImg{
	float: left;
  margin-left: 5%;
}
.product .productImg .imgB_01{
	display: inline-block;
	width: 50rpx;
	height: 81.5rpx;
	background: url(../img/zc_33.png) no-repeat 50% 50%;
	background-size: contain;
  margin-right: 20rpx;
}
.product .productImg .imgB_02{
	display: inline-block;
	width: 60rpx;
	height: 80rpx;
	background: url(../img/zc_34.png) no-repeat 50% 50%;
	background-size: contain;
}
.productNum{
	margin-right: 5%;
	font-size: 26.8rpx;
	line-height: 80rpx;
	float: right;
	color:#666666;
}
/*  */
.details{
	width: 100%;
	background: #FFFFFF;
	font-size: 0rem;
	border-top: 1px solid #CCCCCC;
	border-bottom: 1px solid #CCCCCC;
	margin-bottom: 10rpx;
}
.details .detailsT{
	width: 96%;
	margin-left: 2%;
	overflow: hidden;
	border-bottom: 1px solid #CCCCCC;
}
.details .detailsT>.i{
	display: inline-block;
	width: 25%;
	height: 100rpx;
	background: url(../img/kj_34.png) no-repeat 50% 50%;
	background-size: contain;
	margin: 20rpx 0;
	float: left;
  box-sizing: border-box;
	border: 1rpx solid #999999;
}
.details .detailsT .span{
	display: inline-block;
	width: 60%;
	font-size: 34rpx;
	float: left;
	text-align: center;
	line-height: 140rpx;
}
.details .detailsT .font{
	display: inline-block;
	width: 15%;
	height: 140rpx;
	float: left;
}
.details .detailsT .font .i{
	display: inline-block;
	width: 100%;
	height: 140rpx;
	background: url(../img/kj_35.png) no-repeat 50% 50%;
	background-size:30rpx;	
}
.details .detailsC{
	font-size: 29.84rpx;
	width: 100%;
}
.details .detailsC .detailsC1{
	width: 50%;
	float: left;
	text-align: center;
	margin: 20rpx 0;
}
.details .detailsC .detailsC1 .font{
	color: #666666;
}
.details .detailsB{
	font-size: 32.4rpx;
	width: 100%;
}
.details .detailsB .div{
	width: 30%;
	margin-left: 2.5%;
	padding: 10rpx 0;
	margin-bottom: 20rpx;
	float: left;
	text-align: center;
	outline: 1px solid #CCCCCC;
}
/**/
.explain{
	width: 100%;
} 
.explain .explainT{
	font-size: 32rpx;
	width: 100%;
	margin-bottom: 10rpx;
  line-height: 80rpx;
	background: #FFFFFF;
}
.explain .explainT .div{
	width: 33.333%;
	text-align: center;
	float:left;
}
.explainC1 image{
  width: 100%;
  height: 700rpx;
  display: inline-block;
  float: left;
}
/**/
.FTbutton{
	width: 100%;
	font-size: 0;
	padding: 20rpx 0;
	background: #FFFFFF;
	overflow: hidden;
  position:fixed;
  bottom:0;
}
.FTbutton .FTbutton1,.FTbutton .FTbutton2{
	display: inline-block;
	float: left;
}
.FTbutton .FTbutton1 .span{
	display: inline-block;
	width: 40rpx;
	height: 40rpx;
	margin-left:30rpx;
	/* background: url(../img/kj_26.png) no-repeat 50% 50%;
	background-size: 40rpx; */
}
.FTbutton .FTbutton1{
	margin-top: 5rpx;
	margin-left: 3%;
}
.FTbutton .FTbutton1 .p{
	font-size: 26.4rpx;
	color: #666666;
	margin-top: 5rpx;
}
.FTbutton .FTbutton2{
	font-size: 29.8rpx;
	color: #FFFFFF;
	background:#F98B40;
	text-align: center;
	line-height:90rpx;
	margin-left: 5%;
	width:70%;
}
/* tab切换样式修改 */
.font-deauflt{
  color: #333;
  border-bottom:none;
}
.font-orange{
  color: rgb(245,132,47);
  border-bottom:1rpx solid #f98b40;
}

/* 属性选择滑框 */
.product-property{
position:fixed;
z-index:888;
bottom:-232px;
width:100%;
background:#fff;
height: auto;
}
.product-detail{
width:100%;
display:flex;
}
.product-detail image{
width:30%;
height:200rpx;
margin:3%;
margin-bottom:0;
}
.product-detail-r{
font-size:30rpx;
width:60%;
margin:3%;
margin-left:0;
margin-bottom:0;
}
.property-detail{
  font-size: 30rpx;
  margin:10rpx 3%;
  height:125rpx;
  overflow-x:hidden;
}
.product-num{
font-size:30rpx;
height:50rpx;
width:20%;
float:right;
margin-top:-70rpx;
color:#666;
display:block;
}
.produc-dname{
  line-height: 50rpx;
}
.product-dprice{
  line-height: 50rpx;
  margin-top:12px;
}
.golauchCrowd{
text-align:center;
line-height:80rpx;
font-size:32rpx;
background:rgb(245,132,47);
color:#fff;
width:100%;
}
.pro-icon{
position:fixed;
left:92%;
bottom:-25px;
z-index:999;
opacity: 0;
transition:1.68s;
}
.pros{
  padding: 10rpx 0;
}
.prochance{
  display: inline-block;
  border: 1px solid red;
  text-align: center;
  margin-left:10rpx;
}
.prochance-deflaut{
  display: inline-block;
  border: 1px solid #eee;
  text-align: center;
  margin-left:10rpx;
}
.prochance text{
  display: inline-block;
  margin:0 5px;
  padding-left:8rpx;
}
.prochance-deflaut text{
  display: inline-block;
  margin:0 5px;
  padding-left:8rpx;
  padding-right: 8rpx;
}
.prochecked{
  width: 20rpx;
  height: 20rpx;
  float: right;
  position: relative;
  top: 20rpx;
}
.prochecked-deflaut{
  width: 0rpx;
  height: 0rpx;
  float: right;
  position: relative;
  top: 20rpx;
}
