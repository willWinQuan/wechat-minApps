/*自己留言 begin*/
.myselfMsg{
    margin-top: 20rpx;
    border-top: 2rpx solid #ccc;
    background: #fff;
    margin-bottom: 30rpx;
    border-bottom: 2rpx solid #ccc;
}
.myselfMsg-title{
    display: inherit;
    font-size: 30rpx;
    line-height: 80rpx;
    margin-left: 5%;
    color: #888;
}
.myselfMsg input{
	display: inherit;
    line-height: 80rpx;
