/*地址 end*/


/*选择的商品列表 begin*/

.checkShopList{
	margin-top: 20rpx;
  background:#fff;
	
}


.storeName{
    border-bottom: 2rpx solid #ccc;
    padding-bottom: 13rpx;
    display: flex;
    padding-top:20rpx;
}
.storeName .storeName-icon{
    display: inline-block;
    margin-left: 5%;
    width: 45rpx;
    height: 50rpx;
    background: url('../img/gift.png') no-repeat 50% 50%;
    background-size: contain;
}
#storeName{
    display: inline-block;
    font-size: 32rpx;
    width: 100%;
    color: #888;
    line-height: 50rpx;
    margin-left: 1%;
}
.checkShopList-show{
	display: flex;
    padding-top: 30rpx;
    height: 260rpx;
    border-bottom:1px solid #eee;
    background: #fff;
}
.checkShopList-left{
width:250rpx;
height:250rpx;
margin-left:5%;

}
.checkShopList-right{
	width: 100%;
    margin-right: 5%;
    margin-left: 15rpx;
}
.checkShopList-name{
	font-size: 30rpx;
}
.checkShopList-dec{
	font-size: 30rpx;
    color: rgb(254,132,47);
    display: block;
    line-height: 60rpx;
    height:60rpx;
}
.checkShopList-num{
font-size:35rpx;
color:red;
position:relative;
top:-10rpx;

}
.checkShopList-num2{
	left: 10rpx;
}
.checkShopList-time{
font-size:20rpx;
position:relative;
bottom:-10rpx;
color:#888;
width:111%;
}

/*配送*/
.dispatching{
	font-size: 32rpx;
    line-height: 90rpx;
}
.dispatching-left{
	margin-left: 5%;
}
.dispatching-right{
	float: right;
  margin-right: 5%;
	
}
.dispatching-right text{
  margin-left:5rpx; 
  font-size:28rpx;
  color: #333; 
}
.dispatching-alt{
	width: 20rpx;
    height: 20rpx;
    display: inline-block;
    border-top: solid 1rpx #888;
    border-right: solid 1rpx #888;
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg);
}
/*选择的商品列表 end*/

/*留言 begin*/
.lemsg{
	font-size: 30rpx;
    margin-top: 35rpx;
    line-height: 60rpx;
    padding-bottom: 20rpx;
    background: #fff;
}
.lemsg .text{
  margin-left: 5%;
	color: #888;
}
.lemsg input{
	display: block;
    margin-left: 5%;
    margin-top: 20rpx;
    font-size: 30rpx;
    text-indent: 15rpx;
    line-height: 60rpx;
    width: 90%;
}
.lemsg input::-webkit-input-placeholder, textarea::-webkit-input-placeholder { 
color: #ccc; 
} 
.lemsg input:-moz-placeholder, textarea:-moz-placeholder { 
color: #ccc; 
} 
.lemsg input::-moz-placeholder, textarea::-moz-placeholder { 
color: #ccc; 
} 
.lemsg input:-ms-input-placeholder, textarea:-ms-input-placeholder { 
color: #ccc; 
} 
/*留言 end*/

/*同意协议 begin*/
.sureAgreement{
	margin-top: 130rpx;
    text-align: center;
    font-size: 30rpx;
}
.sureAgreement-icon{
	display: inline-block;
    width: 30rpx;
    height: 30rpx;
    border: 2rpx solid #ccc;
    border-radius: 50%;
    position: relative;
    top: 8rpx;
    margin-right:10rpx;

}
.sureAgreement-text{
	color: #999;
}
.sureAgreement-alt{
	color: rgb(245,132,47);
}
/*同意协议 end*/

/*合计-确认发起  begin*/
.totalSure{
	background: #fff;
  margin-top: 40rpx;
  box-sizing: content-box;
  position:fixed;
  bottom:0;
  width:100%;
}
.totalSure-left{
	font-size: 32rpx;
    margin: 5%;
    margin-bottom: 0;
    float: left;
    padding-bottom: 1%;
}
.toal-Mnum{
	color: rgb(245,132,47);
}
#toal-Mnum{
	color: rgb(245,132,47);
}
.toal-shopNum{
	font-size: 25rpx;
    color: #999;
    line-height: 50rpx;
}
.totalSure-right{
	font-size: 32rpx;
    float: right;
    height: 92rpx;
    line-height: 92rpx;
    width:300rpx;
    background: rgb(245,132,47);
    text-align: center;
    margin-top: 5%;
    color: #fff;
}
/*合计-确认发起 end*/

/*提示框 begin*/
.prompt{
	position: fixed;
    bottom: 20%;
    left: 20%;
    font-size: 32rpx;
    color: #fff;
    background: rgba(74,74,74,1);
    padding: 30rpx 88rpx;
    display: none;
}
/*提示框 end*/

