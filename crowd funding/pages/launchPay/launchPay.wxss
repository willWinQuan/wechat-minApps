page{
  font-family:"Microsoft YaHei",Arial,Helvetica,sans-serif;-webkit-text-size-adjust:none;
  background: rgb(245,245,245);
}
.clear{
  clear: both;
}
/*地址 begin*/
.address{
background:#fff;
}
.address-dec{
padding-top:30rpx;
margin-bottom:0;
padding-bottom:36rpx;


}
#addDec-name{
    margin-left: 5%;
    font-size: 30rpx;
    font-weight: 600;
}
#addDec-number{
    font-size: 30rpx;
    margin-left: 20%;
}
#addDec-msg{
margin-left: 5%;
font-size:30rpx;
color:#868686;
line-height:60rpx;
}
.address-dec .address-decText{
float:right;
font-size:20rpx;
line-height:10rpx;
padding:15rpx 40rpx;
text-align:center;
margin-top:-100rpx;
margin-right: 5%;
background:rgb(254,132,47);
border-radius:60rpx;
color:#fff;

}

.address-editor{
   border-top:1px solid #eee;
}
.addEditor-left{
float:left;
margin-left:5%;
line-height:0;
font-size:32rpx;
padding-bottom:20rpx;
}
.addEditor-left .true-icon{
    display: inline-block;
    position: relative;
    top: 13rpx;
    width: 40rpx;
    height: 40rpx;
    /* border: 1rpx solid #ccc;
    border-radius: 50%; */
    /* background: url(../img/true01.png) no-repeat 50% 50%;
    background-size: cover; */
}
.addEditor-left span{
    margin-left: -20rpx;
    font-size: 32rpx;
}
.addEditor-right{
    float: right;
}
.addEditor-right-li{
    float: left;
    margin: 0 30rpx;
    color: #888;
    font-size: 30rpx;
    line-height: 60rpx;
}
.addEditor-right-li text{
  margin-left: 5rpx;
}
.addEditor-icon,.addDelte-icon{
    display: inline-flex;
    position: relative;
    top: 1.5rpx;
    width: 30rpx;
    height: 30rpx;
}
